local PD = UI:addPage("TargetInfo",3,true,6)

PD:addLabel("Script message","Script Made By RL Team")

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local selectedPlayerName = nil
local dropdown = nil

-- ÿØÿßŸÑÿ© ÿ™ÿ±ÿ¨ÿπ ŸÇÿßÿ¶ŸÖÿ© ÿ£ÿ≥ŸÖÿßÿ° ÿßŸÑŸÑÿßÿπÿ®ŸäŸÜ
local function getPlayerList()
	local list = {}
	for _, player in pairs(Players:GetPlayers()) do
		table.insert(list, player.Name)
	end
	return list
end

-- ÿ•ŸÜÿ¥ÿßÿ° ÿßŸÑÿØÿ±Ÿàÿ® ÿØÿßŸàŸÜ
dropdown = PD:addDropdown("üéØ ÿßÿÆÿ™ÿ± ÿßŸÑŸÑÿßÿπÿ®", getPlayerList(), 1, function(value)
	selectedPlayerName = value
end)

-- ÿ™ÿ≠ÿØŸäÿ´ ÿßŸÑŸÇÿßÿ¶ŸÖÿ© ÿ™ŸÑŸÇÿßÿ¶ŸäŸãÿß ÿπŸÜÿØ ÿØÿÆŸàŸÑ/ÿÆÿ±Ÿàÿ¨ ŸÑÿßÿπÿ®
local function refreshDropdown()
	dropdown:Refresh(getPlayerList(), 1)
end
Players.PlayerAdded:Connect(refreshDropdown)
Players.PlayerRemoving:Connect(refreshDropdown)

-- ÿØÿßŸÑÿ© ÿ™ÿ±ÿ¨ÿπ ÿßŸÑŸÑÿßÿπÿ® ÿßŸÑŸÖÿÆÿ™ÿßÿ±
local function getTarget()
	if selectedPlayerName then
		return Players:FindFirstChild(selectedPlayerName)
	end
end

-- üöÄ ÿ≤ÿ± ÿßŸÑÿ™ŸÜŸÇŸÑ ÿ•ŸÑŸâ ÿßŸÑŸÑÿßÿπÿ®
PD:addButton("üöÄ ÿ™ŸÜŸÇŸÑ ÿ•ŸÑŸâ ÿßŸÑŸÑÿßÿπÿ®", function()
	local target = getTarget()
	if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
		if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
			LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(0, 3, 0)
		end
	end
end)

-- üåÄ ÿ≤ÿ± ŸÅŸÑŸÜŸÇ ÿßŸÑŸÑÿßÿπÿ®
PD:addButton("üåÄ ŸÅŸÑŸÜŸÇ ÿßŸÑŸÑÿßÿπÿ®", function()
	local me = LocalPlayer
	local target = getTarget()
	if not (me and target and target.Character and me.Character) then return end

	local myHRP = me.Character:FindFirstChild("HumanoidRootPart")
	local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
	if not (myHRP and targetHRP) then return end

	myHRP.CFrame = targetHRP.CFrame + Vector3.new(0, 1, 0)

	local fling = Instance.new("BodyAngularVelocity")
	fling.AngularVelocity = Vector3.new(0, 999999, 0)
	fling.MaxTorque = Vector3.new(1e9, 1e9, 1e9)
	fling.P = 10000
	fling.Name = "FlingForce"
	fling.Parent = myHRP

	task.delay(1.5, function()
		if fling and fling.Parent then
			fling:Destroy()
		end
	end)
end)

-- üëÄ Toggle ŸÖÿ¥ÿßŸáÿØÿ© ÿßŸÑŸÑÿßÿπÿ®
local cameraLoop = false
PD:addToggle("üëÅÔ∏è ŸÖÿ¥ÿßŸáÿØÿ© ÿßŸÑŸÑÿßÿπÿ®", function(value)
	cameraLoop = value
	if value == false then
		workspace.CurrentCamera.CameraSubject = LocalPlayer.Character:FindFirstChild("Humanoid") or LocalPlayer.Character
	end
end)

-- ÿ™ÿ≠ÿØŸäÿ´ ÿßŸÑŸÉÿßŸÖŸäÿ±ÿß ÿ™ŸÑŸÇÿßÿ¶ŸäŸãÿß ŸÉŸÑ ŸÑÿ≠ÿ∏ÿ© ŸÑŸà ÿßŸÑÿ™ŸàŸÇŸÑ ÿ¥ÿ∫ÿßŸÑ
task.spawn(function()
	while true do
		task.wait(0.1)
		if cameraLoop then
			local target = getTarget()
			if target and target.Character and target.Character:FindFirstChild("Head") then
				workspace.CurrentCamera.CameraSubject = target.Character:FindFirstChild("Head")
			end
		end
	end
end)

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")

local espBillboards = {}
local highlights = {}
local connections = {}

local function createESP(player)
	if player == Players.LocalPlayer then return end
	if not player.Character or not player.Character:FindFirstChild("Head") then return end
	if espBillboards[player] then return end

	-- ÿßÿ≥ŸÖ ÿßŸÑÿ≠ÿ≥ÿßÿ® ÿ®ÿßŸÑÿ£ÿ≠ŸÖÿ±
	local bb = Instance.new("BillboardGui")
	bb.Name = "ESP_Billboard"
	bb.Adornee = player.Character.Head
	bb.Size = UDim2.new(0, 100, 0, 20)
	bb.StudsOffset = Vector3.new(0, 2, 0)
	bb.AlwaysOnTop = true

	local label = Instance.new("TextLabel", bb)
	label.Size = UDim2.new(1, 0, 1, 0)
	label.BackgroundTransparency = 1
	label.Text = player.Name
	label.TextColor3 = Color3.new(1, 0, 0) -- ÿ£ÿ≠ŸÖÿ±
	label.TextScaled = true
	label.Font = Enum.Font.SourceSansBold

	bb.Parent = player.Character.Head
	espBillboards[player] = bb

	-- Highlight ŸÑÿßŸÖÿπ ŸàŸàÿßÿ∂ÿ≠
	local highlight = Instance.new("Highlight")
	highlight.Name = "ESP_Highlight"
	highlight.Adornee = player.Character
	highlight.FillColor = Color3.fromRGB(255, 255, 255) -- ÿ£ÿ®Ÿäÿ∂ ŸÑÿßŸÖÿπ
	highlight.OutlineColor = Color3.fromRGB(255, 255, 255) -- ÿ•ÿ∑ÿßÿ± ÿ£ÿ®Ÿäÿ∂
	highlight.FillTransparency = 0.1 -- ÿ£ŸÇŸÑ ÿ¥ŸÅÿßŸÅŸäÿ© = ÿ£ŸÉÿ´ÿ± ŸÑŸÖÿπÿßŸÜ
	highlight.OutlineTransparency = 0 -- ÿßŸÑÿ•ÿ∑ÿßÿ± Ÿàÿßÿ∂ÿ≠
	highlight.Parent = player.Character

	highlights[player] = highlight
end

-- ÿ•ÿ≤ÿßŸÑÿ© ŸÉŸÑ ÿßŸÑŸÉÿ¥ŸÅ
local function removeAllESP()
	for _, bb in pairs(espBillboards) do
		if bb and bb.Parent then
			bb:Destroy()
		end
	end
	for _, h in pairs(highlights) do
		if h and h.Parent then
			h:Destroy()
		end
	end
	espBillboards = {}
	highlights = {}
end

-- ÿ™ŸÅÿπŸäŸÑ ÿßŸÑŸÉÿ¥ŸÅ
local function enableESP()
	for _, player in pairs(Players:GetPlayers()) do
		createESP(player)
	end

	table.insert(connections, Players.PlayerAdded:Connect(function(p)
		p.CharacterAdded:Connect(function()
			wait(1)
			createESP(p)
		end)
	end))

	table.insert(connections, Players.PlayerRemoving:Connect(function(p)
		if espBillboards[p] then
			espBillboards[p]:Destroy()
			espBillboards[p] = nil
		end
		if highlights[p] then
			highlights[p]:Destroy()
			highlights[p] = nil
		end
	end))

	table.insert(connections, RunService.RenderStepped:Connect(function()
		for _, player in pairs(Players:GetPlayers()) do
			createESP(player)
		end
	end))

	StarterGui:SetCore("SendNotification", {
		Title = "ESP",
		Text = "‚úÖ ÿ™ŸÖ ÿ™ŸÅÿπŸäŸÑ ÿßŸÑŸÉÿ¥ŸÅ"
	})
end

-- ÿ™ÿπÿ∑ŸäŸÑ ÿßŸÑŸÉÿ¥ŸÅ
local function disableESP()
	removeAllESP()
	for _, conn in pairs(connections) do
		conn:Disconnect()
	end
	connections = {}

	StarterGui:SetCore("SendNotification", {
		Title = "ESP",
		Text = "‚ùå ÿ™ŸÖ ÿ•ŸäŸÇÿßŸÅ ÿßŸÑŸÉÿ¥ŸÅ"
	})
end

-- Toggle ŸÅŸä ÿ™ÿßÿ® PE
PD:addToggle("üîç ŸÉÿ¥ŸÅ ÿßŸÑŸÑÿßÿπÿ®ŸäŸÜ", function(value)
	if value then
		enableESP()
	else
		disableESP()
	end
end)


  
 local SK = UI:addPage("Protection",4,true,6)

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local VirtualUser = game:GetService("VirtualUser")
local LocalPlayer = Players.LocalPlayer

-- ÿ≠ÿßŸÑÿßÿ™ ÿßŸÑÿ™ŸÅÿπŸäŸÑ
local AntiFlingEnabled = false
local AntiRapeEnabled = false
local AntiAFKEnabled = false

-- ‚õî ŸÖÿ∂ÿßÿØ Fling
RunService.Heartbeat:Connect(function()
    if not AntiFlingEnabled then return end

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            for _, obj in pairs(player.Character:GetDescendants()) do
                if obj:IsA("BodyAngularVelocity") or obj:IsA("BodyVelocity") or obj:IsA("AngularVelocity") or obj:IsA("BodyGyro") then
                    obj:Destroy()
                end
            end
        end
    end
end)

-- üîÑ ŸÖÿ∂ÿßÿØ Rape (ŸäŸÑŸÅ ÿ¥ÿÆÿµŸäÿ™ŸÉ ÿ•ÿ∞ÿß ÿ£ÿ≠ÿØ ŸÇÿ±ÿ® ŸÖŸÜ Ÿàÿ±ÿßŸÉ)
RunService.Heartbeat:Connect(function()
    if not AntiRapeEnabled then return end

    local char = LocalPlayer.Character
    if not (char and char:FindFirstChild("HumanoidRootPart")) then return end
    local hrp = char.HumanoidRootPart

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local otherHRP = player.Character.HumanoidRootPart
            local distance = (hrp.Position - otherHRP.Position).Magnitude
            local behind = (hrp.CFrame.LookVector:Dot((otherHRP.Position - hrp.Position).Unit)) < -0.5
            if distance < 5 and behind then
                hrp.CFrame = hrp.CFrame * CFrame.Angles(0, math.rad(180), 0)
            end
        end
    end
end)

-- üí§ ŸÖÿ∂ÿßÿØ AFK
local afkConnection
local function enableAFK()
    afkConnection = LocalPlayer.Idled:Connect(function()
        VirtualUser:Button2Down(Vector2.new(), workspace.CurrentCamera.CFrame)
        task.wait(1)
        VirtualUser:Button2Up(Vector2.new(), workspace.CurrentCamera.CFrame)
    end)
end

local function disableAFK()
    if afkConnection then
        afkConnection:Disconnect()
        afkConnection = nil
    end
end

-- ‚úÖ ÿ£ÿ≤ÿ±ÿßÿ± ÿßŸÑÿ™ŸÅÿπŸäŸÑ
SK:addToggle("üåÄ ŸÖÿ∂ÿßÿØ Fling", function(state)
    AntiFlingEnabled = state
end)

SK:addToggle("üçë ŸÖÿ∂ÿßÿØ Bang", function(state)
    AntiRapeEnabled = state
end)

